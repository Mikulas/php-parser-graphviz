{
    "name": "mikulas/php-parser-graphviz",
    "description": "Graphviz AST dumper for nikic/php-parser.",
    "type": "library",
    "require": {
        "nikic/php-parser": "^3.0"
    },
    "require-dev": {
        "nette/tester": "^1.7",
        "symfony/console": "^3.2"
    },
    "autoload": {
        "psr-4": {
            "Mikulas\\PhpParser\\": "src/Mikulas/PhpParser"
        }
    },
    "suggest": {
        "symfony/console": "to visualize any source file interactively from terminal"
    },
    "bin": ["bin/phpparser-visualize"],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Mikulas Dite",
            "email": "mikulas.dite@gmail.com"
        }
    ],
    "minimum-stability": "stable"
}
